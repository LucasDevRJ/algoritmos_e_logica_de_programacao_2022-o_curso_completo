Algoritmo "consumo"

Var

   distancia, combustivel, consumoMedio : real

Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   
   escreva("Combustível gasto: ")
   leia(combustivel)
   
   consumoMedio <- distancia / combustivel
   
   escreva("Consumo medio = ", consumoMedio:5:3)

Fimalgoritmo