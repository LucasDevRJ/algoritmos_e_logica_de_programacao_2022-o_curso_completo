Algoritmo "funcoes"

Var

   x, y, z, a, b, c, d : real

Inicio

   x <- 2.0
   y <- 3.0
   z <- -7.0
   
   a <- Exp(x, y) //x elevado a y
   b <- RaizQ(y) //pega a raiz quadrade de y
   c <- Pi       //recebe o valor de Pi
   d <- Abs(z)   //valor absoluto de d
   
   escreval(a)
   escreval(b)
   escreval(c)
   escreval(d)

Fimalgoritmo